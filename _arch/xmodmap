clear lock
clear control
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5

! swap [capslock] to be [ctrl]
keycode 66 = Control_L
! swap [win/super] to be [alt]
keycode 133 = Alt_L
! swap [alt] to be [win/super]
keycode 64 = Super_L

add control = Control_L Control_R
add mod1 = Alt_L Alt_R Meta_L
add mod2 = Num_Lock
add mod3 = Hyper_L
add mod4 = Super_L Super_R
add mod5 = Mode_switch ISO_Level3_Shift
